<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:background="@drawable/side_nav_bar"
	android:gravity="bottom"
	android:orientation="vertical"
	android:paddingLeft="@dimen/activity_horizontal_margin"
	android:paddingTop="@dimen/activity_vertical_margin"
	android:paddingRight="@dimen/activity_horizontal_margin"
	android:paddingBottom="@dimen/activity_vertical_margin"
	android:theme="@style/ThemeOverlay.AppCompat.Dark">

	<ImageButton
		android:id="@+id/imageButton"
		android:layout_width="20dp"
		android:layout_height="20dp"
		android:layout_marginTop="40dp"
		android:background="@drawable/ic_richpush_actionbar_back" />

	<ImageView
		android:id="@+id/imageView"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal"
		android:paddingTop="@dimen/nav_header_vertical_spacing"
		app:srcCompat="@drawable/ic_protrait_default" />

	<TextView
		android:id="@+id/nav_textView1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:paddingTop="@dimen/nav_header_vertical_spacing"
		android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

	<TextView
		android:id="@+id/nav_textView2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

	<LinearLayout
		android:id="@+id/user_level_view"
		android:layout_width="match_parent"
		android:layout_height="30dp"
		android:orientation="horizontal">

		<ImageView
			android:id="@+id/vip_view"
			android:layout_width="20dp"
			android:layout_height="10dp"
			android:layout_gravity="center"
			android:layout_marginTop="5dp"
			android:layout_marginRight="5dp"
			android:layout_marginBottom="5dp"
			android:background="@drawable/vip1"
			android:visibility="gone" />

		<ImageView
			android:id="@+id/svip_view"
			android:layout_width="20dp"
			android:layout_height="10dp"
			android:layout_gravity="center"
			android:layout_marginTop="5dp"
			android:layout_marginRight="5dp"
			android:layout_marginBottom="5dp"
			android:background="@drawable/svip1"
			android:visibility="gone" />

		<ImageView
			android:id="@+id/level_hg1"
			android:layout_width="15dp"
			android:layout_height="15dp"
			android:layout_gravity="center"
			android:background="@drawable/qhg"
			android:gravity="center_vertical"
			android:visibility="gone"
			tools:ignore="InefficientWeight" />

		<ImageView
			android:id="@+id/level_hg2"
			android:layout_width="15dp"
			android:layout_height="15dp"
			android:layout_gravity="center"
			android:background="@drawable/qhg"
			android:gravity="center_vertical"
			android:visibility="gone"
			tools:ignore="InefficientWeight" />

		<ImageView
			android:id="@+id/level_hg3"
			android:layout_width="15dp"
			android:layout_height="15dp"
			android:layout_gravity="center"
			android:background="@drawable/qhg"
			android:gravity="center_vertical"
			android:visibility="gone"
			tools:ignore="InefficientWeight" />

		<ImageView
			android:id="@+id/level_ty1"
			android:layout_width="15dp"
			android:layout_height="15dp"
			android:layout_gravity="center"
			android:background="@drawable/qty"
			android:gravity="center_vertical"
			android:visibility="gone"
			tools:ignore="InefficientWeight" />

		<ImageView
			android:id="@+id/level_ty2"
			android:layout_width="15dp"
			android:layout_height="15dp"
			android:layout_gravity="center"
			android:background="@drawable/qty"
			android:gravity="center_vertical"
			android:visibility="gone"
			tools:ignore="InefficientWeight" />

		<ImageView
			android:id="@+id/level_ty3"
			android:layout_width="15dp"
			android:layout_height="15dp"
			android:layout_gravity="center"
			android:background="@drawable/qty"
			android:gravity="center_vertical"
			android:visibility="gone"
			tools:ignore="InefficientWeight" />

		<ImageView
			android:id="@+id/level_yl1"
			android:layout_width="15dp"
			android:layout_height="15dp"
			android:layout_gravity="center"
			android:background="@drawable/qyl"
			android:gravity="center_vertical"
			android:visibility="gone"
			tools:ignore="InefficientWeight" />

		<ImageView
			android:id="@+id/level_yl2"
			android:layout_width="15dp"
			android:layout_height="15dp"
			android:layout_gravity="center"
			android:background="@drawable/qyl"
			android:gravity="center_vertical"
			android:visibility="gone"
			tools:ignore="InefficientWeight" />

		<ImageView
			android:id="@+id/level_yl3"
			android:layout_width="15dp"
			android:layout_height="15dp"
			android:layout_gravity="center"
			android:background="@drawable/qyl"
			android:gravity="center_vertical"
			android:visibility="gone"
			tools:ignore="InefficientWeight" />

		<ImageView
			android:id="@+id/level_qxx1"
			android:layout_width="15dp"
			android:layout_height="15dp"
			android:layout_gravity="center"
			android:background="@drawable/qxx"
			android:gravity="center_vertical"
			android:visibility="gone"
			tools:ignore="InefficientWeight" />

		<ImageView
			android:id="@+id/level_qxx2"
			android:layout_width="15dp"
			android:layout_height="15dp"
			android:layout_gravity="center"
			android:background="@drawable/qxx"
			android:gravity="center_vertical"
			android:visibility="gone"
			tools:ignore="InefficientWeight" />

		<ImageView
			android:id="@+id/level_qxx3"
			android:layout_width="15dp"
			android:layout_height="15dp"
			android:layout_gravity="center"
			android:background="@drawable/qxx"
			android:gravity="center_vertical"
			android:visibility="gone"
			tools:ignore="InefficientWeight" />

		<ImageView
			android:id="@+id/level_bxx"
			android:layout_width="15dp"
			android:layout_height="15dp"
			android:layout_gravity="center"
			android:background="@drawable/bxx"
			android:gravity="center_vertical"
			android:visibility="gone"
			tools:ignore="InefficientWeight" />

	</LinearLayout>

</LinearLayout>

